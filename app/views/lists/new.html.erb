<main>
  <%= link_to 'Back', lists_path, class: 'btn btn-outline-secondary m-2' %>
  <div class="container">
    <h1 class="mt-2 mb-5">New List</h1>
    <%= simple_form_for @list do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, as: :file %>
      <%= f.submit 'Create', class: 'btn btn-secondary' %>
    <% end %>
  </div>
</main>
